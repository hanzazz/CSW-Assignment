<!-- ---------------------------------------------------------manu2---------------------------------------------------------------- -->
    <div id="menu2" class="tab-pane fade">
      <body ng-app="FamApp">
        <div ng-controller="MainController">           
              <table class="table table-striped">
                <thead>
                  <tr> <!-- table row -->
                    <th>ID</th>
                    <th>StudentID</th>
                    <th>Name</th>
                    <th>Room</th>
                    <th>Start Date/Time - End Date/Time</th>
                    <th>Teacher</th>
                    <th>Details</th>
                  </tr>
                </thead>

                <tbody>
                  <tr ng-repeat="project in projects" ng-if="!isEmpty(project)">
                    <td>{{ $index+1 }}</td>
                    <td>{{ project.StudentID }}</td>
                    <td>{{ project.Name }}</td>
                    <td>{{ project.Room }}</td>
                    <td>{{ project.Date }}</td>
                    <td>{{ project.Teacher }}</td>
                    <td>{{ project.Details }}</td>
                    <td><button ng-click="edit(project.id)" class="btn btn-primary">Edit</button></td>
                    <td><button ng-click="delete(project.id)" class="btn btn-danger">Delete</button></td>

                  </tr>
                </tbody>
              </table>
<!-- ---------------------------------------------------------New Booking---------------------------------------------------------------- -->
              <hr>
        <div class="col col-md-6">
          <h2>New Booking</h2>
           <form>
            <div class="form-group">
              <label for="">StudentID</label>
              <input style="width:300px" type="hidden" class="form-control" ng-model="idx">
              <input id="estudent" ng-click="showstudent()" style="width:300px" type="text" class="form-control" placeholder="StudentID" ng-model="project.StudentID" ng-init="project.message=''">
            </div>
            <div class="form-group">
              <label for="">Name</label>
              <input id="ename" ng-click="showname()" style="width:300px" type="text" class="form-control" placeholder="Name" ng-model="project.Name">
            </div>
            <div class="form-group">
              <label for="">Room</label>
              <input id="eroom" ng-click="showroom()" style="width:300px" type="text" class="form-control" placeholder="Room" ng-model="project.Room">
            </div>
            <div class="form-group">
              <label for="">Date/Time</label>
              <input id="edate" ng-click="showdate()" style="width:300px" type="text" class="form-control" placeholder="Date" ng-model="project.Date" name="daterange"/>
            </div>
            <div class="form-group">
              <label for="">Teacher</label>
              <input id="eteacher" ng-click="showteacher()" style="width:300px" type="text" class="form-control" placeholder="Teacher" ng-model="project.Teacher">
            </div>
            <div class="form-group">
              <label for="">Details</label>
              <input id="edetails" ng-click="showdetails()" style="width:300px" type="text" class="form-control" placeholder="Details" ng-model="project.Details">
            </div>  
  
            <button type="submit" class="btn btn-success" ng-click="add()">Submit</button>
          </form>
        </div>
<!-- ---------------------------------------------------------Edit Booking---------------------------------------------------------------- -->
        <div class="col col-md-6">
          <h2>Edit Booking</h2>
          <form>
            <div class="form-group">
              <label for="">StudentID</label>
              <input style="width:300px" type="hidden" class="form-control" ng-model="Editproject.id">
              <input id="lstudent" ng-click="editstudent()" style="width:300px" type="text" class="form-control" placeholder="StudentID" ng-model="Editproject.StudentID" ng-init="Editproject.message=''">
            </div>
            <div class="form-group">
              <label for="">Name</label>
              <input id="lname" ng-click="editname()" style="width:300px" type="text" class="form-control" placeholder="Name" ng-model="Editproject.Name">
            </div>
            <div class="form-group">
              <label for="">Room</label>
              <input id="lroom" ng-click="editroom()" style="width:300px" type="text" class="form-control" placeholder="Room" ng-model="Editproject.Room">
            </div>
            <div class="form-group">
              <label for="">Date/Time</label>
              <input id="edate" ng-click="showdate()" style="width:300px" type="text" class="form-control" placeholder="Date" ng-model="Editproject.Date" name="daterange"/>
            </div>
            <div class="form-group">
              <label for="">Teacher</label>
              <input id="lteacher" ng-click="editteacher()" style="width:300px" type="text" class="form-control" placeholder="Teacher" ng-model="Editproject.Teacher">
            </div>
             <div class="form-group">
              <label for="">Details</label>
              <input id="ldetails" ng-click="editdetails()" style="width:300px" type="text" class="form-control" placeholder="Details" ng-model="Editproject.Details">
            </div>
            <button type="submit" class="btn btn-success" ng-click="update(Editproject.id)">Edit</button>
          </form>
          <br><br><br><br><br><br><br><br>
        </div>
      </div>
